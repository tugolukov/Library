@model System.Collections.Generic.List<Microsoft.AspNetCore.Mvc.Rendering.SelectListItem>

<select id="author-dropdown" name="authorguid" class="form-control mysearch" required>
    <option value="">Выберите автора</option>

    @foreach (var item in Model)
    {
        <option value="@item.Value">@item.Text</option>
    }
</select>
